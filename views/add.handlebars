    <div style="padding-top: 50px;"></div>
	<h1>New member</h1>
	<form class="form-horizontal" id="newmember" method="get" action="/user">
			<fieldset style="margin: 40px;">
			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Name</label>  
			  <div class="col-md-3">
			  <input name="name" type="text" class="form-control input-md" required="true">
			    
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Relationship</label> 
			  <div class="col-md-3">
			  <input name="relationship" type="text" class="form-control input-md" required="true">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Family Side</label>  
			  <div class="col-md-3">
			  <input name="side" type="text" placeholder="Enter 'mom' or 'dad'" class="form-control input-md" required="true">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Gender</label>  
			  <div class="col-md-2">
			  <input name="gender" type="text" class="form-control input-md">  
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Age</label>  
			  <div class="col-md-2">
			  <input name="age" type="text" class="form-control input-md">
			  </div>
			</div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Height</label>  
			  <div class="col-md-2">
			  <input name="height" type="text" class="form-control input-md">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Weight</label>  
			  <div class="col-md-2">
			  <input name="weight" type="text" class="form-control input-md">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Allergies</label>  
			  <div class="col-md-4">
			  <textarea name="allergy" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Sensitivities</label>  
			  <div class="col-md-4">
			  <textarea name="sense" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Heart Conditions</label>  
			  <div class="col-md-4">
			  <textarea name="heart" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>

			<div class="form-group">
			  <label class="col-md-4 control-label">Medications</label>  
			  <div class="col-md-4">
			  <textarea name="med" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Behavioral Problems</label>  
			  <div class="col-md-4">
			  <textarea name="bprob" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>
			

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Emotional Problems</label>  
			  <div class="col-md-4">
			  <textarea name="eprob" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Eye Conditions</label>  
			  <div class="col-md-4">
			  <textarea name="eye" type="text" class="form-control input-md"></textarea>
			  </div>
			</div>
			</fieldset>
			<div class="wrapper">
				<button class="btn btn-danger" type="reset">Reset</button> 
				<input type="submit" class="btn btn-success" id="done" value="Submit">			
				<!--<button class="btn btn-success" id="done" type="submit">Submit</button>-->
			</div>
		</form>

