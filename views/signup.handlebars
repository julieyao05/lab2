    <div style="padding-top: 50px;"></div>
	<h1>Sign up</h1>
		<form class="form-horizontal">
			<fieldset style="margin: 40px;">
			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Username</label>  
			  <div class="col-md-3">
			  <input name="username" type="text" class="form-control input-md" required="">
			    
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Password</label> 
			  <div class="col-md-3">
			  <input name="pwd" type="password" class="form-control input-md" required="">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Name</label>  
			  <div class="col-md-3">
			  <input name="name" type="text" class="form-control input-md" required="">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Gender</label>  
			  <div class="col-md-2">
			  <input name="gender" type="text" class="form-control input-md" required="">  
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Age</label>  
			  <div class="col-md-2">
			  <input name="age" type="text" class="form-control input-md" required="">
			  </div>
			</div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Height</label>  
			  <div class="col-md-2">
			  <input name="height" type="text" class="form-control input-md" required="">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Weight</label>  
			  <div class="col-md-2">
			  <input name="weight" type="text" class="form-control input-md" required="">
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Allergies</label>  
			  <div class="col-md-4">
			  <textarea name="allergy" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Sensitivities</label>  
			  <div class="col-md-4">
			  <textarea name="sense" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Heart Conditions</label>  
			  <div class="col-md-4">
			  <textarea name="heart" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>

			<div class="form-group">
			  <label class="col-md-4 control-label">Medications</label>  
			  <div class="col-md-4">
			  <textarea name="med" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>
			
			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Behavioral Problems</label>  
			  <div class="col-md-4">
			  <textarea name="bprob" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Emotional Problems</label>  
			  <div class="col-md-4">
			  <textarea name="eprob" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>

			<!-- Text input-->
			<div class="form-group">
			  <label class="col-md-4 control-label">Eye Conditions</label>  
			  <div class="col-md-4">
			  <textarea name="eye" type="text" class="form-control input-md" required=""></textarea>
			  </div>
			</div>
			</fieldset>
			<div class="wrapper">
				<button class="btn btn-danger" type="reset">Reset</button> <a href="user" class="btn btn-success">Finish</a>
			</div>
		</form>
	</div>

